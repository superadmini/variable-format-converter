{
  "name": "admin-variable-format-converter",
  "displayName": "Admin Var Circle Format Converter",
  "description": "Convert variables between different naming conventions and circle change format",
  "version": "0.0.9",
  "publisher": "superadmini",
  "icon": "images/app_line.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "camelCase",
    "snake_case",
    "PascalCase",
    "kebab-case",
    "variable",
    "format",
    "converter",
    "naming"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/superadmini/variable-format-converter.git"
  },
  "bugs": {
    "url": "https://github.com/superadmini/variable-format-converter/issues"
  },
  "homepage": "https://github.com/superadmini/variable-format-converter#readme",
  "license": "MIT",
  "activationEvents": [
    "onCommand:adminVariableFormatConverter.convertToCamelCase",
    "onCommand:adminVariableFormatConverter.convertToSnakeCase",
    "onCommand:adminVariableFormatConverter.convertToPascalCase",
    "onCommand:adminVariableFormatConverter.convertToKebabCase",
    "onCommand:adminVariableFormatConverter.convertToUpperCase",
    "onCommand:adminVariableFormatConverter.convertToLowerCase",
    "onCommand:adminVariableFormatConverter.circleChange",
    "onCommand:adminVariableFormatConverter.convertVariable"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "adminVariableFormatConverter.convertToCamelCase",
        "title": "Admin: Convert to camelCase"
      },
      {
        "command": "adminVariableFormatConverter.convertToSnakeCase",
        "title": "Admin: Convert to snake_case"
      },
      {
        "command": "adminVariableFormatConverter.convertToPascalCase",
        "title": "Admin: Convert to PascalCase"
      },
      {
        "command": "adminVariableFormatConverter.convertToKebabCase",
        "title": "Admin: Convert to kebab-case"
      },
      {
        "command": "adminVariableFormatConverter.convertToUpperCase",
        "title": "Admin: Convert to UPPER_CASE"
      },
      {
        "command": "adminVariableFormatConverter.convertToLowerCase",
        "title": "Admin: Convert to lowercase"
      },
      {
        "command": "adminVariableFormatConverter.circleChange",
        "title": "Admin: Circle Change Format"
      },
      {
        "command": "adminVariableFormatConverter.convertVariable",
        "title": "Admin: Convert Variable Format"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "adminVariableFormatConverter.convertVariable",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.36.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
